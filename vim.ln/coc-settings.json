{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "python"],
  "eslint.filetypes": ["javascript", "javascriptreact"],
  "tslint.configFile": "tslint.fix.json",
  "tslint.autoFixOnSave": true,
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "prettier.jsxBracketSameLine": true,
  "prettier.trailingComma": "es5",
  "prettier.singleQuote": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "snippets.userSnippetsDirectory": "~/.dotfiles/cocSnips",
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["~/.dotfiles/UltiSnips"],
  "snippets.ultisnips.usePythonx": false,
  "diagnostic.locationlist": true,
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "black",
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.venvFolders": ["anaconda3/envs"]
}
